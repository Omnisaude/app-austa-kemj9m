NEXT_PUBLIC_API_URL=https://api.austa.health
NEXT_PUBLIC_API_VERSION=v1
NEXT_PUBLIC_API_TIMEOUT=30000
NEXT_PUBLIC_API_RETRY_ATTEMPTS=3

# Authentication
NEXT_PUBLIC_AUTH_DOMAIN=auth.austa.health
NEXT_PUBLIC_AUTH_CLIENT_ID=your_client_id
AUTH_CLIENT_SECRET=your_client_secret
NEXT_PUBLIC_AUTH_AUDIENCE=https://api.austa.health
JWT_SECRET_KEY=your_jwt_secret
JWT_REFRESH_SECRET=your_refresh_secret
JWT_EXPIRY=15m
REFRESH_TOKEN_EXPIRY=7d

# Virtual Care
NEXT_PUBLIC_TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
NEXT_PUBLIC_TWILIO_API_KEY=your_twilio_api_key
TWILIO_API_SECRET=your_twilio_api_secret
NEXT_PUBLIC_TWILIO_ROOM_TYPE=group
NEXT_PUBLIC_TWILIO_VIDEO_CODEC=VP8
NEXT_PUBLIC_TWILIO_RECORDING_ENABLED=false

# Storage
NEXT_PUBLIC_S3_BUCKET=austa-health-files
NEXT_PUBLIC_S3_REGION=us-east-1
S3_ACCESS_KEY=your_s3_access_key
S3_SECRET_KEY=your_s3_secret_key
NEXT_PUBLIC_S3_ENDPOINT=https://s3.amazonaws.com
NEXT_PUBLIC_MAX_FILE_SIZE=10485760
NEXT_PUBLIC_ALLOWED_FILE_TYPES=.pdf,.jpg,.png,.doc,.docx

# Analytics
NEXT_PUBLIC_ANALYTICS_ID=your_analytics_id
NEXT_PUBLIC_MIXPANEL_TOKEN=your_mixpanel_token
NEXT_PUBLIC_ANALYTICS_ENABLED=true
NEXT_PUBLIC_ERROR_TRACKING_ENABLED=true
NEXT_PUBLIC_USER_TRACKING_ENABLED=true

# Security
NEXT_PUBLIC_ENCRYPTION_KEY=your_encryption_key
NEXT_PUBLIC_ENABLE_MFA=true
NEXT_PUBLIC_ENABLE_BIOMETRIC=true
CSP_REPORT_ONLY=false
ENABLE_RATE_LIMITING=true
RATE_LIMIT_WINDOW=15m
RATE_LIMIT_MAX_REQUESTS=100
NEXT_PUBLIC_PASSWORD_MIN_LENGTH=12
NEXT_PUBLIC_SESSION_TIMEOUT=30m

# Feature Flags
NEXT_PUBLIC_ENABLE_VIRTUAL_CARE=true
NEXT_PUBLIC_ENABLE_MARKETPLACE=true
NEXT_PUBLIC_ENABLE_CLAIMS=true
NEXT_PUBLIC_ENABLE_HEALTH_RECORDS=true
NEXT_PUBLIC_ENABLE_WEARABLES=true
NEXT_PUBLIC_ENABLE_PRESCRIPTIONS=true
NEXT_PUBLIC_ENABLE_LAB_RESULTS=true
NEXT_PUBLIC_ENABLE_EMERGENCY_CARE=true