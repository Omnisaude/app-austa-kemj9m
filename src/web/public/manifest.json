{
  "name": "AUSTA SuperApp",
  "short_name": "AUSTA",
  "description": "Comprehensive digital healthcare platform for telemedicine, health records, insurance, and wellness services",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#FFFFFF",
  "theme_color": "#0066CC",
  "orientation": "portrait",
  "icons": [
    {
      "src": "/icons/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any maskable"
    },
    {
      "src": "/icons/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any maskable"
    }
  ],
  "categories": [
    "healthcare",
    "medical",
    "lifestyle",
    "telemedicine"
  ],
  "scope": "/",
  "shortcuts": [
    {
      "name": "Virtual Care",
      "url": "/virtual-care",
      "description": "Start or join a virtual care session"
    },
    {
      "name": "Health Records",
      "url": "/health-records",
      "description": "View your health records"
    },
    {
      "name": "Claims",
      "url": "/claims",
      "description": "Manage insurance claims"
    }
  ],
  "related_applications": [],
  "prefer_related_applications": false,
  "screenshots": [
    {
      "src": "/screenshots/home-mobile.png",
      "sizes": "1170x2532",
      "type": "image/png",
      "platform": "mobile",
      "label": "Home Screen"
    },
    {
      "src": "/screenshots/virtual-care-mobile.png",
      "sizes": "1170x2532",
      "type": "image/png",
      "platform": "mobile",
      "label": "Virtual Care Interface"
    }
  ],
  "permissions": [
    "camera",
    "microphone",
    "notifications",
    "geolocation"
  ],
  "protocol_handlers": [
    {
      "protocol": "web+austacall",
      "url": "/virtual-care/%s"
    }
  ],
  "iarc_rating_id": "",
  "share_target": {
    "action": "/share-target",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "media",
          "accept": ["image/*", "application/pdf"]
        }
      ]
    }
  },
  "file_handlers": [
    {
      "action": "/open-file",
      "accept": {
        "application/pdf": [".pdf"],
        "image/*": [".png", ".jpg", ".jpeg"]
      }
    }
  ],
  "background_color": "#FFFFFF",
  "theme_color": "#0066CC",
  "version": "1.0.0",
  "manifest_version": 2
}